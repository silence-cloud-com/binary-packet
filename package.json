    "deserialize",
    "encode",
    "decode",
    "packer",
    "unpacker",
    "packet",
    "message",
    "data",
    "fast",
    "lightweight",
    "efficient",
    "buffer",
    "arraybuffer",
    "dataview"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silence-cloud-com/binary-packet.git"
  },
  "author": "silence-cloud.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "colors": "^1.4.0",
    "eslint": "^10.0.0",
    "msgpackr": "^1.11.8",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "restructure": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=20"
  }
